public class UserProfile {
    private String firstName;
    private String lastName;
    private String username;
    private String userPassword;
    private double userWeight;
    private double userHeight;
    private int userMedAmt;

    //Initializes user
    public UserProfile(String first, String last, String user, String password, double weight, double height, int medAmt){
        firstName=first;
        lastName=last;
        username=user;
        userPassword=password;
        userWeight=weight;
        userHeight=height;
        userMedAmt=medAmt;
    }

    //Sets Profile attributes
    public void setFirstName(String firstName){
        this.firstName=firstName;
    }
    public void setLastName(String lastName){
        this.lastName=lastName;
    }
    public void setUserName(String username){
        this.username=username;
    }
    public void setUserPassword(String userPassword){
        this.userPassword=userPassword;
    }
    public void setUserWeight(double userWeight){
        this.userWeight=userWeight;
    }
    public void setUserHeight(double userHeight){
        this.userHeight=userHeight;
    }
    public void setUserMedAmt(int userMedAmt){
        this.userMedAmt=userMedAmt;
    }

    //Gets Profile attributes
    public String getFirstName(){
        return firstName;
    }
    public String getLastName(){
        return lastName;
    }
    public String getUsername(){
        return username;
    }
    public String getUserPassword(){
        return userPassword;
    }
    public double getUserWeight(){
        return userWeight;
    }
    public double getUserHeight(){
        return userHeight;
    }
    public int getUserMedAmt(){
        return userMedAmt;
    }
}
